{
    "name": "News Scan Extension",
    "version": "0.1",
    "description": "Scan the web for other news articles relating to the one you have!",
    "background": {
	"scripts": ["background.js"],
	"persistent": false
    },
    "browser_action": {
	"default_popup": "popup.html"
    },
    "permissions": [
	"activeTab",
	"tabs",
    	"http://*/*",
    	"https://*/",
      "http://127.0.0.1:5000/"
    ],
    "manifest_version": 2
}
