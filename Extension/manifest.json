{
    "name": "News Scan Extension",
    "version": "0.1",
    "description": "Scan the web for other news articles relating to the one you have!",
    "background": {
	"persistent": false,
	"scripts": ["jquery-3.3.1.min.js", "background.js"]
    },
    "browser_action": {
	"default_popup": "popup.html"
    },
    "permissions": [
	"activeTab",
	"tabs",
    	"http://*/*",
    	"https://*/*"
    ],
    "manifest_version": 2
}